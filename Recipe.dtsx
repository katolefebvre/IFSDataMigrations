    {
        public void Main()
        {
            string[] tables = new string[] { 
                "RECIPE_STRUCTURE_HEAD_TAB",
                "RECIPE_STRUCT_ALTERNATE_TAB",
                "RECIPE_STRUCTURE_TAB"
            };

            try
            {
                SqlConnection myADONETConnection = new SqlConnection();
                myADONETConnection = (SqlConnection)(Dts.Connections["DataMigration"].AcquireConnection(Dts.Transaction) as SqlConnection);

                foreach (string table in tables)
                {
                    string delete_query = "delete From [dbo].[" + table + "]";
                    SqlCommand data_cmd = new SqlCommand(delete_query, myADONETConnection);

                    data_cmd.ExecuteNonQuery();
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch
            {
                // whoops
            }
        }

        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
    }
